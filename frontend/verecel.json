{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "admin-ss-footwear-business-website.vercel.app"
        }
      ],
      "dest": "/public/pages/admin/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/pages/customer/$1"
    }
  ]
}
